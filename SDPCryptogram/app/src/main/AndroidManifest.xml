<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.sdpcryptogram">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".NewUserActivity" />
        <activity android:name=".PlayerHomeScreenActivity" />
        <activity android:name=".CreateNewCryptogramActivity" />
        <activity android:name=".CryptogramStatActivity" />
        <activity android:name=".ListOfCompletedActivity" />
        <activity android:name=".ListOfUnSolvedActivity" />
        <activity android:name=".PlayCryptogram" />
        <activity android:name=".PlayCryptogramActivity" />
        <activity android:name=".SingleCryptogramStatActivity"></activity>
    </application>

</manifest>